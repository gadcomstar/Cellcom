        color: #888;
    }

.datepick-month-row {
    clear: left;
}

.datepick-month {
    float: left;
    width: 15em;
    border: 1px solid #83c948;
    text-align: center;
}

.datepick-month-header, .datepick-month-header select, .datepick-month-header input {
    height: 1.5em;
    background-color: #83c948;
    color: #fff;
    font-weight: bold;
}

